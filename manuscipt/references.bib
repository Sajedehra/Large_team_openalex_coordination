@article{goring_improving_2014,
	title = {Improving the culture of interdisciplinary collaboration in ecology by expanding measures of success},
	volume = {12},
	copyright = {© The Ecological Society of America},
	issn = {1540-9309},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1890/120370},
	doi = {10.1890/120370},
	abstract = {Interdisciplinary collaboration is essential to understand ecological systems at scales critical to human decision making. Current reward structures are problematic for scientists engaged in interdisciplinary research, particularly early career researchers, because academic culture tends to value only some research outputs, such as primary-authored publications. Here, we present a framework for the costs and benefits of collaboration, with a focus on early career stages, and show how the implementation of novel measures of success can help defray the costs of collaboration. Success measures at team and individual levels include research outputs other than publications, including educational outcomes, dataset creation, outreach products (eg blogs or social media), and the application of scientific results to policy or management activities. Promotion and adoption of new measures of success will require concerted effort by both collaborators and their institutions. Expanded measures should better reflect and reward the important work of both disciplinary and interdisciplinary teams at all career stages, and help sustain and stimulate a collaborative culture within ecology.},
	language = {en},
	number = {1},
	urldate = {2025-01-23},
	journal = {Frontiers in Ecology and the Environment},
	author = {Goring, Simon J and Weathers, Kathleen C and Dodds, Walter K and Soranno, Patricia A and Sweet, Lynn C and Cheruvelil, Kendra S and Kominoski, John S and Rüegg, Janine and Thorn, Alexandra M and Utz, Ryan M},
	year = {2014},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1890/120370},
	pages = {39--47},
	file = {Full Text PDF:C\:\\Users\\20235753\\Zotero\\storage\\NWIBZF2T\\Goring et al. - 2014 - Improving the culture of interdisciplinary collaboration in ecology by expanding measures of success.pdf:application/pdf;Snapshot:C\:\\Users\\20235753\\Zotero\\storage\\4ACRVCUP\\120370.html:text/html},
}

